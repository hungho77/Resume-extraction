# LLM Base URL
# For vLLM: http://localhost:8000/v1
# For OpenAI: https://api.openai.com/v1
# For Anthropic: https://api.anthropic.com/v1

# LLM API Key (required for OpenAI/Anthropic, not needed for vLLM)
LLM_BASE_URL=http://localhost:8000/v1
LLM_API_KEY="EMPTY"
LLM_MODEL=Qwen/Qwen3-8B

# LLM_BASE_URL=https://api.openai.com/v1
# LLM_API_KEY=your-api-key
# LLM_MODEL=gpt-4o

LLM_MAX_TOKENS=8096
LLM_TEMPERATURE=0.7
LLM_TENSOR_PARALLEL_SIZE=1

# DocLing Configuration
DOCLING_USE_OCR=true

# vLLM Configuration
VLLM_HOST=localhost
VLLM_PORT=8000
